.header-list{
    display: flex;
}
.section-hi__content{
    width: 100%;
    display: flex;
    justify-content: center;
}
.header-item{
    color: #ffffff;
    font-size: 16px;
    font-weight: 400;
    line-height: 30px;
    margin-right: 60px;

     &:last-child{
         margin-right: 0;
     }
     a{
         &:hover{
             text-decoration: underline;
         }
     }
}